files = [ f
	for f in glob('*.py')
	if not os.path.exists(f[:-3]+'=py') \
	   and f != 'python-head.py'
	]
files.append('version.py')
files.extend(glob('*/*.py'))
files.extend(map(lambda filename:filename[:-3]+'.pyc', files))
files.sort()
htmls = glob('html/*')
htmls.sort()
dependon(files + htmls)
: Makefile
rm -f %(target)s
zip -q9 %(target)s %(deplist_pre)s
